<ErrorNull>
	
<remove xpath="/entity_classes/entity_class[@name='playerMale']/property[contains(@name,'ItemsOnEnterGame')]"/>

<!-- increase unencumbered carrying capacity from 27 to 45 -->
<set xpath="/entity_classes/entity_class[@name='playerMale']/effect_group/passive_effect[@name='CarryCapacity']/@value">45</set>

<!-- set player max hp to 150 -->
<set xpath="/entity_classes/entity_class[@name='playerMale']/effect_group/passive_effect[@name='HealthMax']/@value">150</set>

<!-- remove XP deficit from death -->
<set xpath="/entity_classes/entity_class[@name='playerMale']/effect_group/passive_effect[@name='ExpDeficitPerDeathPercentage'][@value='.25']/@value">0</set>

<!-- player backpack on death despawns immediately to prevent spectator item use -->
<set xpath="/entity_classes/entity_class[@name='Backpack']/property[@name='TimeStayAfterDeath']/@value">0</set>

<!-- tags onSelfRespawn property for easier modification -->
<setattribute xpath="/entity_classes/entity_class[@name='playerMale']/effect_group/triggered_effect[@trigger='onSelfRespawn']" name="id">respawn</setattribute>

<!-- remove vanilla buffs to prevent stats restore after death respawn -->
<set xpath="/entity_classes/entity_class[@name='playerMale']/effect_group/triggered_effect[@id='respawn']/@buff">buffStatusCheck01,buffStatusCheck02</set>


<insertAfter xpath="/entity_classes/entity_class[@name='playerMale']/effect_group/triggered_effect[@trigger='onSelfRespawn']">
	
	<!-- set game time to soem minutes prior to 7:00am, then stop time progression -->
	<triggered_effect trigger="onSelfEnteredGame" action="CallGameEvent" event="enAction_timeBackward10"/>
	<triggered_effect trigger="onSelfEnteredGame" action="AddBuff" buff="enBuffTimeHold"/>
	<triggered_effect trigger="onSelfEnteredGame" action="AddBuff" buff="enBuffShowZombieKillCounter"/>
	<triggered_effect trigger="onSelfEnteredGame" action="AddBuff" buff="enBuffShowTotalScore"/>
	<triggered_effect trigger="onSelfEnteredGame" action="AddBuff" buff="enBuffShowStageNumber"/>
	
	<triggered_effect trigger="onSelfEnteredGame" action="ModifyScreenEffect" intensity="1" fade="0.5" effect_name="Greyscale"/>
	
	<!-- set cvar that tracks current stage number to 1 -->
	<triggered_effect trigger="onSelfEnteredGame" action="ModifyCVar" cvar="enCvarStage" operation="set" value="1"/>
	
	<!-- execute certain actions based on current time -->
	<triggered_effect trigger="onSelfEnteredGame" action="AddBuff" buff="enBuffTimeTracker"/>
	
	<!-- check if player is knocked out -->
	<triggered_effect trigger="onSelfEnteredGame" action="AddBuff" buff="enBuffKoCheck"/>
	
	<!-- remove all injury buffs except for burning and bleeding -->
	
    <passive_effect name="BuffResistance" operation="base_set" value="999" tags="buffFatiguedTrigger,buffArmSprainedCHTrigger,buffLegSprainedCHTrigger,buffLaceration,buffInfectionCatch,buffAbrasionCatch,buffInjuryStunned01CHTrigger,buffArmBroken,buffLegBroken,buffLegSprained,buffArmSprained,buffInjuryStunned00,buffInjuryStunned01,buffInjuryStunned02,buffInjuryStunned03"/>
	
	<!-- remove effects of thirst and hunger -->
	<passive_effect name="WaterChangeOT" operation="base_add" value="10"/>
	<passive_effect name="FoodChangeOT" operation="base_add" value="10"/>

	<!-- player becomes spectator upon death and respawn -->
	<triggered_effect trigger="onSelfRespawn" action="AddBuff" target="self" buff="enBuffSpectator"/>

</insertAfter>


<!-- Prevent any random loot drops from any zombies -->
<set xpath="/entity_classes/entity_class/property[@name='LootDropProb']/@value">0.0</set>

<!-- how many seconds before zombie and animal corpse despawns -->
<set xpath="/entity_classes/entity_class[@name='zombieTemplateMale']/property[@name='TimeStayAfterDeath']/@value">3</set>
<set xpath="/entity_classes/entity_class[@name='animalTemplateHostile']/property[@name='TimeStayAfterDeath']/@value">3</set>

<!-- make zombies immune to bleeding, to counteract the engine unable to provide coin rewards for bleeding death -->
<append xpath="/entity_classes/entity_class[@name='zombieTemplateMale']/effect_group[@name='Base Effects']/passive_effect[@name='BuffResistance']/@tags">,buffInjuryBleeding</append>


